    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "redux-devtools-extension": "^2.13.8",
    "rimraf": "^3.0.2",
    "scaffdog": "^0.3.0",
    "typescript": "4.1"
  }
